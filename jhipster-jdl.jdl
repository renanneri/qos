entity User {
	firstName String required
    lastName String
    gender String 
    mailAdress String required
    group String
}

entity Group {
	name String
    description String
}

entity Mail {
	subject String
    recipient String
	message String
    group String
}

relationship ManyToOne {
	User{group} to Group
}

relationship ManyToMany{
	Mail{group} to Group
}

dto * with mapstruct

